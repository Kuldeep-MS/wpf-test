<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>FixedDocViewer</AssemblyName>
    <LinkSubsystem>console</LinkSubsystem>
    <OutputType>Library</OutputType>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="FixedDocViewer.cs" />
    <Compile Include="FixedDocSource.cs" />
    <Compile Include="FixedDocSourceConverter.cs" />
    <Compile Include="FixedDocData.cs" />
    <Compile Include="FixedGrid.cs" />
    <Compile Include="FixedCell.cs" />
    <Compile Include="FixedPageRect.cs" />
    <Compile Include="FixedUtil.cs" />
    <Compile Include="DoubleUtil.cs" />
    <None Include="FGrid.xaml">
      <Link>DrtFiles\FixedDocViewer\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Include="FixedDocViewer.xaml">
      <Link>DrtFiles\FixedDocViewer\%(FileName)%(Extension)</Link>
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
  </ItemGroup>
</Project>
