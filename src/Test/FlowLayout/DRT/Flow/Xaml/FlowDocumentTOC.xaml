<FlowDocument xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" ColumnWidth="800">
	<Paragraph FontSize="24pt" TextAlignment="Center">
		<Run>FlowDocument with links to other documents.</Run>
	</Paragraph>
	<Paragraph TextAlignment="Center">
		<Hyperlink Name="htoc" NavigateUri="#toc">Jump to TOC</Hyperlink>
	</Paragraph>
	<Paragraph BreakPageBefore="True" FontSize="24pt" TextAlignment="Center" Name="toc">
		<Run>TOC</Run>
	</Paragraph>
	<Paragraph TextAlignment="Center" LineHeight="24pt">
		<Hyperlink Name="hfirstdoc" NavigateUri="FlowDocumentComplex.xaml">First document</Hyperlink><LineBreak />
		<Hyperlink Name="hseconddoc" NavigateUri="FlowDocumentPlain.xaml">Second document</Hyperlink><LineBreak />
		<Hyperlink Name="hfirstpage" NavigateUri="#htoc">First page</Hyperlink><LineBreak />
	</Paragraph>
</FlowDocument>
